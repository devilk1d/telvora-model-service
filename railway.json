{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "runtime": "V2",
    "numReplicas": 1
  },
  "nixpacks": {
    "phases": {
      "setup": {
        "nixPkgs": ["python310"]
      }
    },
    "environment": {
      "PYTHON_VERSION": "3.10"
    }
  }
}
